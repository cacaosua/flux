  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart",
        "--verbose",
      ],
      "showMemoryUsage": true
    },
    {
      "name": "Launch staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "args": [
        "--flavor",
        "staging",
        "--target",
        "lib/main_staging.dart",
      ],
      "showMemoryUsage": true
    },
    {
      "name": "Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": [
        "--flavor",
        "production",
        "--target",
        "lib/main_production.dart",
      ],
      "showMemoryUsage": true
    },
    {
      "name": "Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_storyboard.dart",
        "--verbose",
      ],
      "showMemoryUsage": true
    },
    {
      "name": "Examples - Counter",
      "type": "dart",
      "request": "launch",
      "cwd": "examples/counter",
      "program": "lib/main.dart"
    },
    {
      "name": "Examples - Todos",
      "type": "dart",
      "request": "launch",
      "cwd": "examples/todos",
      "program": "lib/main.dart"
    },
    {
      "name": "Examples - TicTacToe",
      "type": "dart",
      "request": "launch",
      "cwd": "examples/tic_tac_toe",
      "program": "lib/main.dart"
    }
  ]
}