{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "all",
  // "dart.env": {
  //     "CHROME_EXECUTABLE": "/Applications/Microsoft Edge.app/Contents/MacOS/Microsoft Edge"
  // },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
  },
  "explorerExclude.backup": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/coverage.lcov": false,
    "*.lcov": false,
    "coverage.lcov": false,
    "**/*.lcov": false,
    "**/codecov.yml": false,
    "*.yml": false,
    "codecov.yml": false,
    "**/*.yml": false,
    "**/coverage": false,
    "coverage": false,
    "**/.dart_tool": false,
    ".dart_tool": false,
    ".DS_Store": false,
    "**/ios": false,
    "ios": false,
    "**/android": false,
    "android": false,
    "**/.vscode": false,
    ".vscode": false,
    "**/.github": false,
    ".github": false,
    "**/analysis_options.yaml": false,
    "analysis_options.yaml": false,
  }
}